  "explorer.trash.command": "mv --backup=t ~/.local/share/Trash",
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "i": false, // cancel default mapkey
    "<tab>": "actionMenu",
    "e": "open",
    "t": "open:tab",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "f": "search",
    "F": "searchRecursive",
    "X": "systemExecute"
  }
}
